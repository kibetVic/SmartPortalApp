@model SmartPortalApp.Models.CourseSubject

@{
    ViewData["Title"] = "Details";
}

<div>
    <h4>Course Subject</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Course Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CourseId)
        </dd>
        <dt class = "col-sm-2">
            Subject Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SubjectId)
        </dd>
        <dt class = "col-sm-2">
            GradeId
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GradeId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.CourseSubjectsId" class="btn btn-warning">Edit</a>
    <a asp-action="Index" class="btn btn-primary">Back</a>
</div>
